<table class="table">
  <%= form_with(scope: :projects, url: projects_path, method: :post, local: true) do |p| %>
    <thead>
      <tr>
        <td><h3>New Project</h3></td>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= p.label :project_name %></td>
        <td><%= p.text_field :project_name %></td>
      </tr>
      <tr>
        <td><%= p.submit %></td>
        <td><%= link_to "cancel", projects_path, method: :get %></td>
      </tr>
    </tbody>
  <% end %>
</table>